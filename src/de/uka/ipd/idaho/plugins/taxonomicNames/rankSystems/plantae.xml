<rankSystem name="plantae" code="ICN" domain="Botany">
	<rankGroup name="domain">
		<rank name="domain" abbreviations="dom."/>
	</rankGroup>
	<rankGroup name="kingdom">
		<rank name="superKingdom" abbreviations="superreg." synonyms="superRegnum"/>
		<rank name="kingdom" abbreviations="reg." synonyms="regnum"/>
		<rank name="subKingdom" abbreviations="subreg." synonyms="subRegnum"/>
		<rank name="infraKingdom" abbreviations="infrareg." synonyms="infraRegnum"/>
	</rankGroup>
	<rankGroup name="phylum">
		<rank name="superPhylum" abbreviations="superphyl."/>
		<rank name="phylum" abbreviations="phyl." suffix="phyta"/>
		<rank name="subPhylum" abbreviations="subphyl." suffix="phytina"/>
		<rank name="infraPhylum" abbreviations="infraphyl."/>
	</rankGroup>
	<rankGroup name="class">
		<rank name="superClass" abbreviations="supercl." synonyms="superClassis"/>
		<rank name="class" abbreviations="cl." suffix="opsida" synonyms="classis"/>
		<rank name="subClass" abbreviations="subcl." suffix="idae" synonyms="subClassis"/>
		<rank name="infraClass" abbreviations="infracl." synonyms="infraClassis"/>
	</rankGroup>
	<rankGroup name="order">
		<rank name="superOrder" abbreviations="superord." suffix="anae" synonyms="superOrdo"/>
		<rank name="order" abbreviations="ord." suffix="ales" synonyms="ordo"/>
		<rank name="subOrder" abbreviations="subord." suffix="ineae" synonyms="subOrdo"/>
		<rank name="infraOrder" abbreviations="infraord." synonyms="infraOrdo"/>
	</rankGroup>
	<rankGroup name="family">
		<rank name="superFamily" abbreviations="superfam." suffix="acea" synonyms="superFamilia"/>
		<rank name="family" abbreviations="fam." suffix="aceae" synonyms="familia"/>
		<rank name="subFamily" abbreviations="subfam." suffix="oideae" synonyms="subFamilia"/>
		<rank name="infraFamily" abbreviations="infrafam." synonyms="infraFamilia"/>
		<rank name="superTribe" abbreviations="supertrib." synonyms="superTribus"/>
		<rank name="tribe" abbreviations="trib." suffix="eae" synonyms="tribus"/>
		<rank name="subTribe" abbreviations="subtrib." suffix="inae" synonyms="subTribus"/>
		<rank name="infraTribe" abbreviations="infratrib." synonyms="infraTribus"/>
	</rankGroup>
	<rankGroup name="genus">
		<rank name="genus" abbreviations="gen."/>
		<rank name="subGenus" abbreviations="subg.;subgen." epithetTemplate="(@epithet)"/>
		<rank name="infraGenus" abbreviations="infragen." epithetTemplate="(@epithet)"/>
		<rank name="section" abbreviations="sect." epithetTemplate="(sect. @epithet)" synonyms="sectio"/>
		<rank name="subSection" abbreviations="subsect." epithetTemplate="(subsect. @epithet)" synonyms="subSectio"/>
		<rank name="series" abbreviations="ser." epithetTemplate="(ser. @epithet)"/>
		<rank name="subSeries" abbreviations="subser." epithetTemplate="(subser. @epithet)"/>
	</rankGroup>
	<rankGroup name="species">
		<rank name="speciesAggregate" abbreviations="aggr."/>
		<rank name="species" abbreviations="sp.;spec."/>
		<rank name="subSpecies" abbreviations="ssp.;subsp.;subspec." epithetTemplate="subsp. @epithet"/>
		<rank name="infraSpecies" abbreviations="infrasp.;infraspec."/>
		<rank name="variety" abbreviations="var.;v." epithetTemplate="var. @epithet" synonyms="varietas"/>
		<rank name="subVariety" abbreviations="svar.;subvar.;subv." epithetTemplate="subvar. @epithet" synonyms="subVarietas"/>
		<rank name="form" abbreviations="fm.;f." epithetTemplate="fm. @epithet" synonyms="forma"/>
		<rank name="subForm" abbreviations="subfm.;subf." epithetTemplate="subfm. @epithet" synonyms="subForma"/>
	</rankGroup>
</rankSystem>
